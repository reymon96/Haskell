--crear una función que permita realizar la multiplicación de matices, calcular la determinante de una matriz y calcular la transpuesta de una matriz.

main =  print "Funciones con matrices: resp 1 : Multiplicacion) "

matriz = ([1, 2, 3, 4, 5], [6, 7, 8, 9, 0])

printMat = do
	print matriz

resp :: Int -> String
resp m
 | m <= 1 = "Multiplicacion"
 | m <= 2 = "Determinante"
 | m <= 3 = "Transpuesta"
 | otherwise = "Los rangos deben ser del 1 al 3"


